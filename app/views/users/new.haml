.big-content	
	.header-link.left.active= link_to "CREATE", "/users/new"
	.divider.left= "|"
	.header-link.left.inactive= link_to "BOOKS", "/users/index/1"
	.clearboth
	.content
		.form
			= form_for :user, :url => "/users/create" do |f|
				= f.text_field :username, :id => "username", :placeholder => "username", :autocomplete => "off"
				= f.submit "Create User", :id => "submit_btn", :class => "submit_btn"
				.right.error