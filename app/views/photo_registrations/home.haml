#reservation_index= link_to "Registrations Index", "/photo_registrations/index"
- @books.each do |book|
	.content
		.title= book.title
		.book_code= book.book_code
		.description= book.description
		.author= book.author
		.image_url= book.image_url
		.release_date= book.release_date
		.rating= book.rating
		.create_new_reservation= link_to "Regist it", "/photo_registrations/new/#{book.id}", :method => "post"